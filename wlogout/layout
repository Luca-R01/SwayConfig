{
    "label" : "lock",
    "action" : "systemctl --user start swaylock.service",
    "text" : "Blocca",
    "keybind" : "l"
}
{
    "label" : "hibernate",
    "action" : "systemctl hibernate",
    "text" : "Iberna",
    "keybind" : "h"
}
{
    "label" : "logout",
    "action" : "swaymsg exit",
    "text" : "Esci",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff && clear",
    "text" : "Spegni",
    "keybind" : "s"
}
{
    "label" : "suspend",
    "action" : "systemctl suspend && systemctl --user start swaylock.service",
    "text" : "Sospendi",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot && clear",
    "text" : "Riavvia",
    "keybind" : "r"
